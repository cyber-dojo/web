<script>
'use strict';
$(() => {

  const review = cd.review;

  review.fadeIn = (onComplete) => {
    cd.kata.page.fadeOut('fast', () => {
      review.page.fadeIn('fast', () => {
        onComplete();
      });
    });
  };

  review.fadeOut = (onComplete) => {
    review.page.fadeOut('fast', () => {
      cd.kata.page.fadeIn('fast', () => {
        onComplete();
      });
    });
  };

});
</script>
