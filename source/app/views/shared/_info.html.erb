<img id="info-button" src="/images/info.png" />

<%= render partial: 'shared/dashboard' %>
<%= render partial: 'shared/feedback' %>
<%= render partial: 'shared/help' %>
<%= render partial: 'shared/hotkeys' %>
<%= render partial: 'shared/micro_services' %>
<%= render partial: 'shared/traffic_lights' %>

<script>
'use strict';
$(() => {

  const $button = $('#info-button');
  const $buttons = () => $('.info-button');
  cd.createTip($button, 'toggle information buttons on/off');
  $button.clickToggle(() => $buttons().fadeIn(), () => $buttons().fadeOut());
  $button.click();

});
</script>
