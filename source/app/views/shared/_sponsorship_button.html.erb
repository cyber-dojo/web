<button type="button" class="sponsorship">sponsorship</button>

<script>
'use strict';
$(() => {

  const $button = $('button.sponsorship');

  $button.click(() => openSponsorshipDialog());

  const openSponsorshipDialog = () => {
    $('#sponsorship-dialog').dialog({
              width: 440,
           autoOpen: true,
      closeOnEscape: true,
              modal: true,
              title: cd.dialogTitle('sponsorship'),
        beforeClose: () => cd.kata.editor.refocus()
    });
  };

});
</script>
