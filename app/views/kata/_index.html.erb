<%# An incrementing index is held in the browser. %>
<%# This allow out-of-sync mobbing detection.     %>
<input name="index" type="hidden" value="0">

<script>
'use strict';
$(() => {

  const $index = $("input[name='index']");

  cd.kata.index = 0;

  cd.kata.incrementIndex = () => {
    $index.val(cd.kata.index += 1);
  };

});
</script>
