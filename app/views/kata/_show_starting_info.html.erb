<script>
'use strict';
$(() => {

  cd.showStartingInfoDialog = () => {
    if (cd.kata.index === 1 && !turnItOff()) {
      cd.trafficLightsButton().click();
    }
  };

  const turnItOff = () => {
    return cd.env.STARTING_INFO_DIALOG === 'off' ||
      cd.kata.manifest().starting_info_dialog === 'off';
  };

});
</script>
