
<div class="fake-filenames"></div>

<script type="text/javascript">
'use strict';
$(() => {

  const fakeFilenames = $('.fake-filenames');
  ['stdout','stderr','status'].forEach((filename) => {
    const div = $('<div>', {
        class: 'fake filename',
           id: `radio_${filename}`,
         text: filename
    });
    div.click(() => {
      $('.filename').removeClass('selected');
      cd.loadFile(filename);
      div.addClass('selected');
    });
    fakeFilenames.append(div);
  });

});
</script>
