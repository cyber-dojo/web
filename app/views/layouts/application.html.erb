          "FORK_BUTTON":          "<%= @env['FORK_BUTTON'] %>",
          "STARTING_INFO_DIALOG": "<%= @env['STARTING_INFO_DIALOG'] %>",
          "REPL":                 "<%= @env['REPL'] %>",
          "THEME":                "<%= @env['THEME'] %>",
          "COLOUR":               "<%= @env['COLOUR'] %>",
          "PREDICT":              "<%= @env['PREDICT'] %>"
        },
        kata: {
          id: "<%= @id %>",
          filenames:{},
          tabs:{},
