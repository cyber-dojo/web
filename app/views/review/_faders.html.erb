<script>
'use strict';
$(() => {

  cd.review.fadeIn = (onComplete) => {
    cd.kata.page.fadeOut('fast', () => {
      cd.review.page.fadeIn('fast', () => {
        onComplete();
      });
    });
  };

  cd.review.fadeOut = (onComplete) => {
    cd.review.page.fadeOut('fast', () => {
      cd.kata.page.fadeIn('fast', () => {
        onComplete();
      });
    });
  };

});
</script>
