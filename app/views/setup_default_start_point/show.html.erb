      <td colspan="3" align="right">
        <%= render partial: 'heading' %>
      </td>
    </tr>
    <tr class="valign-top">
    </tr>
    <tr class="valign-top">
      <td id="info-container">
        <%= render partial: 'info' %>
      </td>
