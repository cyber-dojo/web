
<%= render partial: 'shared/start_point_chooser' %>

<div id="setup-start-point-page">

  <div id="pull-spinner"></div>

  <table>
    <tr class="valign-top">
      <td colspan="3" align="right">
        <%= render partial: 'heading' %>
      </td>
    </tr>
    <tr class="valign-top">
    </tr>
    <tr class="valign-top">
      <td id="info-container">
        <%= render partial: 'info' %>
      </td>
      <td id="start-points-major-container">
        <div class="title">language?</div>
        <hr/>
        <%= render partial: 'shared/start_points_major_list' %>
      </td>
      <td id="start-points-minor-container">
        <div class="title">test-framework?</div>
        <hr/>
        <%= render partial: 'shared/start_points_minor_list' %>
      </td>
    </tr>
    <tr>
      <td colspan="3" align="right">
        <%= render partial: 'choose_exercise_button' %>
      </td>
    </tr>
  </table>

</div>

<%= render partial: 'shared/app_bar_short',
    locals: { help_url: "http://blog.cyber-dojo.org/p/learn-more.html" } %>


